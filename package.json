{
	"name": "rekisteri",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"db:start": "docker compose up",
		"db:reset": "docker compose down -v",
		"db:push": "drizzle-kit push",
		"db:push:force": "drizzle-kit push --force",
		"db:seed": "tsx --env-file=.env src/lib/server/db/seed.ts",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.78",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.2",
		"drizzle-seed": "^0.3.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"lucide-svelte": "^0.473.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.19.1",
		"svelte-check": "^4.1.4",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11",
		"vite-plugin-kit-routes": "^0.7.5",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.1.1",
		"@fontsource-variable/roboto-mono": "^5.1.1",
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"drizzle-orm": "^0.38.4",
		"mode-watcher": "^0.5.0",
		"postgres": "^3.4.5"
	},
	"packageManager": "pnpm@9.15.4"
}
