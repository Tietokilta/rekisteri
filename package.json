{
	"name": "rekisteri",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"db:start": "docker compose up",
		"db:reset": "docker compose down -v",
		"db:push": "drizzle-kit push",
		"db:push:force": "drizzle-kit push --force",
		"db:seed": "tsx --env-file=.env src/lib/server/db/seed.ts",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"i18n:validate": "inlang validate --project project.inlang",
		"i18n:lint": "inlang lint --project project.inlang"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.19.0",
		"@inlang/cli": "^2.18.1",
		"@inlang/message-lint-rule-empty-pattern": "^1.4.8",
		"@inlang/message-lint-rule-identical-pattern": "^1.5.9",
		"@inlang/message-lint-rule-missing-translation": "^1.4.8",
		"@inlang/message-lint-rule-valid-js-identifier": "^1.0.8",
		"@inlang/message-lint-rule-without-source": "^1.4.8",
		"@inlang/plugin-m-function-matcher": "^1.0.6",
		"@inlang/plugin-message-format": "^3.1.1",
		"@playwright/test": "^1.50.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.0",
		"bits-ui": "1.0.0-next.78",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.2",
		"drizzle-seed": "^0.3.0",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.0",
		"globals": "^15.14.0",
		"lucide-svelte": "^0.474.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.19.3",
		"svelte-check": "^4.1.4",
		"sveltekit-superforms": "^2.23.1",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^4.0.0",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11",
		"vite-plugin-kit-routes": "^0.7.6",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.1.1",
		"@fontsource-variable/roboto-mono": "^5.1.1",
		"@inlang/paraglide-js": "2.0.0-beta.14",
		"@inlang/paraglide-sveltekit": "1.0.0-beta.4",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"drizzle-orm": "^0.38.4",
		"mode-watcher": "^0.5.1",
		"postgres": "^3.4.5"
	},
	"packageManager": "pnpm@9.15.4",
	"engines": {
		"node": "^22.13.0"
	},
	"pnpm": {
		"patchedDependencies": {
			"@inlang/message-lint-rule-empty-pattern": "patches/@inlang__message-lint-rule-empty-pattern.patch",
			"@inlang/message-lint-rule-identical-pattern": "patches/@inlang__message-lint-rule-identical-pattern.patch",
			"@inlang/message-lint-rule-missing-translation": "patches/@inlang__message-lint-rule-missing-translation.patch",
			"@inlang/message-lint-rule-valid-js-identifier": "patches/@inlang__message-lint-rule-valid-js-identifier.patch",
			"@inlang/message-lint-rule-without-source": "patches/@inlang__message-lint-rule-without-source.patch"
		}
	}
}
