{
  "name": "rekisteri",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "[ -n \"$NIX_BUILD_TOP\" ] || (svelte-kit sync && pnpm i18n:generate)",
    "dev": "concurrently -n vite,i18n -c cyan,magenta \"vite dev\" \"pnpm i18n:watch\"",
    "build": "pnpm i18n:generate && vite build",
    "flake:upgrade": "docker run --rm -v $(pwd):/app -w /app nixpkgs/nix-flakes sh -c \"git config --global --add safe.directory /app && nix flake update\"",
    "flake:build": "docker run --rm -v $(pwd):/app -w /app nixpkgs/nix-flakes sh -c \"git config --global --add safe.directory /app && nix build .#docker\"",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:e2e": "playwright test",
    "test:integration": "vitest run",
    "test": "pnpm run test:e2e",
    "db:start": "docker compose up",
    "db:clear": "docker compose down -v",
    "db:reset": "pnpm db:clear && pnpm db:start -d --wait && pnpm db:migrate && pnpm db:seed",
    "db:seed": "tsx --env-file=.env src/lib/server/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "i18n:generate": "typesafe-i18n --no-watch",
    "i18n:watch": "typesafe-i18n",
    "typesafe-i18n": "typesafe-i18n"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@internationalized/date": "^3.11.0",
    "@lucide/svelte": "^0.563.1",
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-node": "^5.5.2",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/table-core": "^8.21.3",
    "@testcontainers/postgresql": "^11.11.0",
    "@types/node": "^24.10.12",
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.5.2",
    "@types/qrcode": "^1.5.6",
    "bits-ui": "^2.15.5",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.9",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.3.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte-check": "^4.3.6",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "testcontainers": "^11.11.0",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-plugin-kit-routes": "^1.0.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/roboto-mono": "^5.2.8",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "drizzle-orm": "^0.45.1",
    "html5-qrcode": "^2.3.8",
    "mailgun.js": "^12.7.0",
    "mode-watcher": "^1.1.0",
    "node-cron": "^4.2.1",
    "papaparse": "^5.5.3",
    "postgres": "^3.4.8",
    "qrcode": "^1.5.4",
    "runed": "^0.37.1",
    "stripe": "^20.3.1",
    "svelte": "^5.50.0",
    "typesafe-i18n": "^5.26.2",
    "valibot": "^1.2.0"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=22.0.0 <25.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "axios": "^1.13.0",
      "esbuild@<=0.24.2": ">=0.25.0",
      "cookie@<0.7.0": ">=0.7.0",
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "validator@<13.15.20": ">=13.15.20",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "valibot@>=0.31.0 <1.2.0": ">=1.2.0",
      "validator@<13.15.22": ">=13.15.22"
    }
  }
}
